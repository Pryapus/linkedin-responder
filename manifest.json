{
    "manifest_version": 3,
    "name": "LinkedIn Chat Logger",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://www.linkedin.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["popup.js"]
      }
    ],
    "action": {
        "default_popup": "popup.html"
    }
  }
  